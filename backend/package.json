{
    "name": "shopping-webapp",
    "version": "0.0.0",
    "description": "E-commerce web server",
    "type": "module",
    "devDependencies": {
        "@eslint/js": "~9.17",
        "@types/bcryptjs": "^2.4.6",
        "@types/cookie-parser": "^1.4.9",
        "@types/cors": "^2.8.19",
        "@types/dotenv": "^6.1.1",
        "@types/eslint__js": "~8.42",
        "@types/express": "^5.0.3",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/mongodb": "^4.0.6",
        "@types/mongoose": "^5.11.96",
        "@types/node": "^20.19.2",
        "@typescript-eslint/parser": "~8.19",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/eslint-plugin": "~1.1",
        "eslint": "~9.17",
        "eslint-config-prettier": "~9.1",
        "globals": "~15.14",
        "jest": "^30.0.3",
        "prettier": "~3.4",
        "rimraf": "~6.0",
        "ts-api-utils": "~2.0",
        "ts-jest": "^29.4.0",
        "typescript": "~5.7",
        "typescript-eslint": "~8.19",
        "vitest": "^3.2.4"
    },
    "scripts": {
        "start": "node build/src/main.js",
        "clean": "rimraf coverage build tmp",
        "prebuild": "npm run lint",
        "build": "tsc -p tsconfig.json",
        "build:watch": "tsc -w -p tsconfig.json",
        "build:release": "npm run clean && tsc -p tsconfig.release.json",
        "lint": "eslint .",
        "test": "vitest run unit --config __tests__/vitest.config.ts",
        "test:coverage": "vitest run unit --config __tests__/vitest.config.ts --coverage.enabled --coverage.all",
        "prettier": "prettier \"{src,__{tests}__}/**/*.{ts,mts}\" --config .prettierrc --write",
        "prettier:check": "prettier \"{src,__{tests}__}/**/*.{ts,mts}\" --config .prettierrc --check",
        "test:watch": "vitest unit"
    },
    "author": "Shreyash",
    "license": "Apache-2.0",
    "dependencies": {
        "bcryptjs": "^3.0.2",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^17.0.0",
        "express": "^5.1.0",
        "jsonwebtoken": "^9.0.2",
        "mongodb": "^6.17.0",
        "mongoose": "^8.16.1",
        "tslib": "~2.8"
    }
}
